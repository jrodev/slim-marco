{% block content %}
    <h4 class="text-center">FICHA TÉCNICA DEL ESTABLECIMIENTO DE SALUD</h4>
    <form id="frmSaveEstab" method="post" enctype="multipart/form-data" autocomplete='off'>
        <div id="thisContainer">
            <div class="row">
                <div class="col-xl-3 col-md-6">
                    <div class="card" table-name="establecimiento">
                        <div class="card-body">
                            <h6 class="card-title">DATOS GENERALES</h6>
                            <div class="form-row">
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Codigo:</label>
                                    <br /><span>{{ estab.codigo }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Nombre Establecimiento</label>
                                    <br /><span>{{ estab.nombre }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Direccion</label>
                                    <br /><span>{{ estab.direccion }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Periodo</label>
                                    <br /><span>{{ estab.periodo }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-0">
                                    <label class="font-weight-bold mb-0">Categoria</label>
                                    <br /><span>{{ estab.categoria_nom }}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-xl-3 col-md-6">
                    <div class="card" table-name="establecimiento">
                        <!--img class="card-img-top img-thumbnail" src="..." alt="Card image cap"-->
                        <div class="card-body">
                            <h6 class="card-title">UBICACIÓN</h6>
                            <div class="form-row">
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Region</label>
                                    <br /><span>{{ estab.region_id }}</span>
                                    <select class="form-control required" id="region_id" name="region_id" placeholder="Region">
                                        <option value="">Region ...</option>
                                        <option value="1">Costa</option>
                                        <option value="2">Sierra</option>
                                        <option value="3">Selva</option>
                                    </select>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Departamento</label>
                                    <br /><span>{{ estab.departamento_nom }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Provincia</label>
                                    <br /><span>{{ estab.provincia_nom }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2 mb-0">
                                    <label class="font-weight-bold mb-0">Distrito</label>
                                    <br /><span>{{ estab.distrito_nom }}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-xl-3 col-md-6">
                    <div class="card" table-name="establecimiento">
                        <!--img class="card-img-top img-thumbnail" src="..." alt="Card image cap"-->
                        <div class="card-body">
                            <h6 class="card-title">RED</h6>
                            <div class="form-row">
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Diris / Diresa</label>
                                    <br /><span>{{ estab.red_dirisdiresa }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Red</label>
                                    <br /><span>{{ estab.red_red }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Microred</label>
                                    <br /><span>{{ estab.red_microred }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Institución</label>
                                    <br /><span>{{ estab.red_institucion }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2 mb-0">
                                    <label class="font-weight-bold mb-0">Área Geográfica</label>
                                    <br /><span>{{ estab.red_areageografica }}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-xl-3 col-md-6">
                    <div class="card" table-name="establecimiento">
                        <!--img class="card-img-top img-thumbnail" src="..." alt="Card image cap"-->
                        <div class="card-body">
                            <h6 class="card-title">INFORMACIÓN GENERAL DEL ESTABLECIMIENTO</h6>
                            <div class="form-row">
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Año puesta en marcha</label>
                                    <br /><span>{{ estab.gral_aniomarcha }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">N° de camas (Internamiento/Hospitalización)</label>
                                    <br /><span>{{ estab.gral_nrocamas }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Población Beneficiaria</label>
                                    <br /><span>{{ estab.gral_poblacbenefic }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Inversión en Infraestructura</label>
                                    <br /><span>{{ estab.gral_inversionInfra }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2 mb-0">
                                    <label class="font-weight-bold mb-0">Inversión en Equipamiento</label>
                                    <br /><span>{{ estab.gral_inversionequip }}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

