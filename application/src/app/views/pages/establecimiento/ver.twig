                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Instalaciones Eléctricas</label>
                                    {% set pars = estados[estab.conserv_instaelect]  %}
                                    <br /><span class="{{ pars.class }}">{{ pars.text }}</span>
                                </div>
                                <div class="form-group col-md-12 mb-2">
                                    <label class="font-weight-bold mb-0">Instalaciones Sanitarias</label>
                                    {% set pars = estados[estab.conserv_instasanit]  %}
                                    <br /><span class="{{ pars.class }}">{{ pars.text }}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

